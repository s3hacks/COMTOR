    $('#home').backstretch([
      a, 
        ],  {duration: 2000, fade: 750});
    });
  })

      $(document).ready( function() {
        $('#hide').delay(2500).fadeOut();
      });

$( "#home" ).mousemove(function() {
  $('#hide').fadeIn( 100 );
});

$( "#home" ).mouseleave(function() {
  $('#hide').fadeOut(500);
});

</script>
